      "title": "Effects of Epigenetics (Anika Morokhovich)",
      "status": "publish",
      "imageURL": "",
      "lockedImageURL": "",
      "mediaType": "text",
      "mediaFormat": "",
      "mediaDuration": 0,
      "description": "",
      "behaviour": "new-window",
      "typeData": {
        "linkMetadata": null,
        "progress": [
          {
            "group": "viewed",
